<Window x:Class="JellyTetris.Windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JellyTetris.Windows"
        mc:Ignorable="d"
        Background="#18272b"
        Title="MainWindow"
        WindowStartupLocation="CenterScreen"
        Height="800"
        Width="1000">
    <Grid>
        <TextBlock x:Name="ModelingTextBlock" Foreground="Gray" HorizontalAlignment="Left" VerticalAlignment="Top" />
        <local:FieldGrid x:Name="_fieldGrid" Background="#181b1c" />
    </Grid>
</Window>
